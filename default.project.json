{
	"name": "roblox-ts-game",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$path": "models/ReplicatedStorage.rbxmx",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$path": "models/StarterPlayer.rbxmx",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Lighting": {
			"$path": "models/Lighting.rbxmx"
		},
		"MaterialService": {
			"$path": "models/MaterialService.rbxmx"
		},
		"Teams": {
			"$path": "models/Teams.rbxx"
		},
		"StarterGui": {
			"$path": "models/StarterGui.rbxmx"
		},
		"ReplicatedFirst": {
			"$path": "models/ReplicatedFirst.rbxmx"
		},
		"Workspace": {
			"$path": "models/Workspace.rbxmx"
		},
		"ServerStorage": {
			"$path": "models/ServerStorage.rbxmx"
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"TextChatService": {
			"$className": "TextChatService",
			"$properties": {
				"ChatVersion": "TextChatService"
			}
		}
	}
}
